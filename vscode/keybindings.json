// Place your key bindings in this file to override the defaults
[
    {
        "key": "ctrl+x g",
        "command": "magit.status"
    },
    {
        "key": "alt+x g",
        "command": "-magit.status"
    },
    {
        "key": "ctrl+x alt+g",
        "command": "magit.file-popup",
        "when": "activeEditor"
    },
    {
        "key": "alt+x alt+g",
        "command": "-magit.file-popup",
        "when": "activeEditor"
    },
    {
        "key": "ctrl+x shift+g",
        "command": "magit.dispatch"
    },
    {
        "key": "alt+x ctrl+g",
        "command": "-magit.dispatch"
    },
    {
        "key": "ctrl+shift+t",
        "command": "extension.railsGoToSpec",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+cmd+y",
        "command": "-extension.railsGoToSpec",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+x ctrl+o",
        "command": "-emacs-mcx.isearchExit",
        "when": "editorFocus && findWidgetVisible && !isComposing"
    },
    {
        "key": "ctrl+x ctrl+o",
        "command": "-emacs-mcx.deleteBlankLines",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+t",
        "command": "editor.action.revealDefinition",
        "when": "editorHasDefinitionProvider && editorTextFocus"
    },
    {
        "key": "f12",
        "command": "-editor.action.revealDefinition",
        "when": "editorHasDefinitionProvider && editorTextFocus"
    },
    {
        "key": "cmd+f12",
        "command": "-editor.action.revealDefinition",
        "when": "editorHasDefinitionProvider && editorTextFocus && isWeb"
    },
    {
        "key": "shift+alt+t",
        "command": "editor.action.goToReferences",
        "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
    },
    {
        "key": "shift+f12",
        "command": "-editor.action.goToReferences",
        "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
    },
    {
        "key": "alt+,",
        "command": "workbench.action.navigateBack",
        "when": "canNavigateBack"
    },
    {
        "key": "ctrl+-",
        "command": "-workbench.action.navigateBack",
        "when": "canNavigateBack"
    },
    {
        "key": "ctrl+t",
        "command": "-editor.action.transposeLetters",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "key": "alt+r",
        "command": "Rails:Navigation"
    },
    {
        "key": "alt+.",
        "command": "-Rails:Navigation"
    },
    {
        "key": "ctrl+t",
        "command": "workbench.action.quickOpen"
    },
    {
        "key": "ctrl+x ctrl+r",
        "command": "workbench.action.quickOpenPreviousRecentlyUsedEditor",
    },
    {
        "key": "ctrl+x f",
        "command": "extension.dired.open",
        "when": "!inDebugRepl"
    },
    {
        "key": "ctrl+x f",
        "command": "-extension.dired.open",
        "when": "editorTextFocus && !inDebugRepl"
    },
    {
        "key": "ctrl+x ctrl+f",
        "command": "workbench.action.quickOpen",
        "when": "!terminalFocus"
    },
    {
        "key": "ctrl+x ctrl+f",
        "command": "-workbench.action.quickOpen",
        "when": "!terminalFocus"
    },
    {
        "key": "cmd+k cmd+m",
        "command": "-workbench.action.toggleMaximizeEditorGroup",
        "when": "editorPartMaximizedEditorGroup || editorPartMultipleEditorGroups"
    },
    {
        "key": "shift+cmd+j",
        "command": "-find-it-faster.findFiles"
    },
    {
        "key": "cmd+0",
        "command": "-workbench.action.focusSideBar"
    },
    {
        "key": "ctrl+x ctrl+j",
        "command": "workbench.view.explorer",
        "when": "viewContainer.workbench.view.explorer.enabled"
    },
    {
        "key": "shift+cmd+e",
        "command": "-workbench.view.explorer",
        "when": "viewContainer.workbench.view.explorer.enabled"
    },
    {
        "key": "ctrl+x p",
        "command": "projectManager.listProjects"
    },
    {
        "key": "alt+cmd+p",
        "command": "-projectManager.listProjects"
    },
    {
        "key": "ctrl+x j",
        "command": "-workbench.action.togglePanel",
        "when": "!terminalFocus"
    },
    {        
        "key": "ctrl+x ctrl+j",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "sideBarFocus"
    },
    {
        "key": "r",
        "command": "renameFile",
        "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
    },
    {
        "key": "enter",
        "command": "-renameFile",
        "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
    },
    {
        "key": "ctrl+r",
        "command": "-workbench.action.quickOpenNavigateNextInRecentFilesPicker",
        "when": "inQuickOpen && inRecentFilesPicker"
    },
    {
        "key": "alt+r",
        "command": "-emacs-mcx.moveToWindowLineTopBottom",
        "when": "config.emacs-mcx.useMetaPrefixAlt && editorTextFocus"
    },
    {
        "key": "ctrl+shift+/",
        "command": "redo"
    },
    {
        "key": "shift+cmd+z",
        "command": "-redo"
    }
]